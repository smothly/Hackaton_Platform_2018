<div class = 'container club_write'>
  <h1>동아리 생성</h1>
  
  <%= form_tag "/club", method: "post", multipart: true do %>
    <input type = "hidden" value = '<%= form_authenticity_token%>' name ='authenticity_token'></input>
    <h6>동아리명</h6>
    <input type = "text" class = 'title' name = "club[name]"></input>
    <h6>동아리 설명</h6>
    <input type = "text" class = 'inform' name = "club[content]"></input>
    <h6>동아리 분류</h6>
    <select class = 'category' name = "club[category]">
      <option value="예술">예술</option>
      <option value="운동">운동</option>
      <option value="봉사">봉사</option>
      <option value="종교">종교</option>
      <option value="학술">학술</option>
    </select>
    <h6>동아리 사진</h6>
    <input type = "file" class = 'file' name = "club[mainimage]" %></input>
    <input type = "submit"></input>
  <% end %>
</div>
